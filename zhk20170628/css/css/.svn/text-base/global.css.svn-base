.sr-only {
    position: absolute;
    width: 1px;
    height: 1px;
    padding: 0;
    margin: -1px;
    overflow: hidden;
    clip: rect(0, 0, 0, 0);
    border: 0;
}

.sr-only-focusable:active,
.sr-only-focusable:focus {
    position: static;
    width: auto;
    height: auto;
    margin: 0;
    overflow: visible;
    clip: auto;
}

body,
html {
    position: relative;
    width: 100%;
    padding: 0;
    margin: 0;
    -webkit-font-smoothing: antialiased;
    font-family: 'Lantinghei SC', 'hiragino sans GB', 'Microsoft YaHei', 'Helvetica Neue', Helvetica, Arial, sans-serif;
}

input,
select,
textarea,
button {
    outline: none;
    font-family: 'Lantinghei SC', 'hiragino sans GB', 'Microsoft YaHei', 'Helvetica Neue', Helvetica, Arial, sans-serif;
}

input[type=text] {
    border: 1px solid #666;
    width: 100%;
    font-size: 14px;
}

select {
    -moz-appearance: none;
    -webkit-appearance: none;
    background: url("../img/arrow.png") no-repeat scroll right center transparent;
    padding-right: 20px;
    border: 1px solid #666;
    font-size: 14px;
    border-radius: 0;
}

select::-ms-expand {
    display: none;
}

h1,
h2,
h3,
h4 {
    margin: 20px 0;
    -webkit-font-smoothing: antialiased;
    font-weight: 300;
}

h1 {
    font-weight: 800;
    font-size: 25px;
}

ul {
    list-style: none;
    padding: 0;
    margin: 0;
}

a {
    text-decoration: none;
    outline: none;
    color: #333;
    font-family: 'hiragino sans GB', 'Microsoft YaHei', 'Helvetica Neue', Helvetica, Arial, sans-serif;
    font-size: 14px;
    color: #000000;
}

.container .bread_crumbs {
    width: 100%;
    margin: 0 auto;
    line-height: 30px;
    font-size: 12px;
    height: 30px;
    border-bottom: 1px solid #ccc;
}

.container .bread_crumbs li {
    line-height: 35px;
    margin: 0 5px;
    float: left;
}

.container .bread_crumbs li a {
    color: #666;
}

.container .bread_crumbs li i {
    color: #666;
}

.container .bread_crumbs li:first-child {
    margin: 0 5px 0 0;
}


