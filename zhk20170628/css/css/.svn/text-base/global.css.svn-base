table {
    width: 100%;
    border-collapse: collapse;
    border-top: 2px solid #a41b21;
    border-bottom: 2px solid #a41b21;
    margin: 20px 0;
}

table thead {
    background: #f0f0f0;
    font-weight: bold;
    border-bottom: 1px solid #a499a5;
}

table thead td,
table thead th {
    border-right: 1px solid #a499a5;
}

table thead td:last-child,
table thead th:last-child {
    border-right: 0;
}

table th,
table td {
    font-size: 14px;
    padding: 5px;
    text-align: center;
}

table th {
    font-weight: bold;
    font-size: 14px;
}

table td.ctrl {
    white-space: nowrap;
}

table tbody tr {
    border-bottom: 1px solid #a499a5;
}

table tbody tr td {
    border-right: 1px solid #ccc;
}

table tbody tr td a {
    color: #175fba;
    text-decoration: underline;
    font-weight: 600;
}

table tbody tr td:last-child {
    border-right: 0;
}

table tbody tr td h3,
table tbody tr td p {
    padding: 0;
    margin: 0;
    line-height: 22px;
}

table caption {
    font-size: 12px;
    line-height: 25px;
}

table caption span {
    display: inline-block;
    margin-right: 20px;
}


