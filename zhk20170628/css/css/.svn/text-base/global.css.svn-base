/*——————————————————————————————————————个人工作室左边框架样式——————————————————————————————————————————*/

.banner .content .admin_nav,
.banner .content .user_nav {
    width: 195px;
    margin: 0 15px 0 0;
    border-right: 1px solid #a41b21;
    padding-right: 15px;
    display: table-cell;
    vertical-align: top;
}

.banner .content .admin_nav h2,
.banner .content .user_nav h2 {
    font-size: 18px;
    font-weight: bold;
    border-bottom: 1px solid #a41b21;
    line-height: 35px;
    margin: 0 0 5px;
    padding: 0;
}

.banner .content .admin_nav ul.main_list,
.banner .content .user_nav ul.main_list {
    padding: 10px 0;
}

.banner .content .admin_nav ul.main_list li,
.banner .content .user_nav ul.main_list li {
    font-size: 14px;
    font-weight: bold;
    color: #000;
    line-height: 30px;
    padding: 0;
}

.banner .content .admin_nav ul.main_list li a,
.banner .content .user_nav ul.main_list li a {
    color: #a41b21;
    display: block;
    line-height: 30px;
    padding: 0 10px;
    border-bottom: 1px solid #ccc;
}

.banner .content .admin_nav ul.main_list li.active a,
.banner .content .user_nav ul.main_list li.active a {
    color: #a41b21;
    background-color: #f0f0f0;
    border: 0 none;
}

.banner .content .admin_nav ul.main_list li i,
.container .banner .user_nav ul.main_list li i {
    color: #a499a5;
    font-size: 14px;
    margin-left: 10px;
}

.banner .content .admin_nav ul.main_list li ul.sub_list,
.banner .content .user_nav ul.main_list li ul.sub_list {
    padding: 10px 10px 10px 20px;
}

.banner .content .admin_nav ul.main_list li ul.sub_list li,
.banner .content .user_nav ul.main_list li ul.sub_list li {
    line-height: 22px;
    font-size: 12px;
}

.banner .content .admin_nav ul.main_list li ul.sub_list li a,
.banner .content .user_nav ul.main_list li ul.sub_list li a {
    color: #666;
    background-color: transparent;
    border: 0 none;
    font-weight: 600;
    line-height: 22px;
}

.banner .content .admin_nav ul.main_list li ul.sub_list li.active a,
.banner .content .user_nav ul.main_list li ul.sub_list li.active a {
    color: #a41b21;
    background-color: transparent;
    border: 0 none;
    font-weight: bold;
}

.banner .content .admin_cont {
    width: calc(100% - 246px);
    padding-left: 20px;
    display: table-cell;
    vertical-align: top;
}

.banner .content .admin_cont h2 {
    font-weight: 600;
}

.banner .content.admin_content {
	height: 850px;
    display: table;
}

.container .content .half_cont {
    float: left;
    width: calc((100% - 20px) / 2);
    margin-right: 10px;
}

.container .footer {
    position: relative;
    width: 100%;
    border-top: 1px solid #a41b21;
    margin-top: 20px;
}

.select_cont {
    padding: 20px;
    background-color: #f0f0f0;
    width: auto;
    display: table;
    margin: 0 auto 20px;
}

.select_cont .select_input {
    display: table-cell;
    white-space: nowrap;
    vertical-align: middle;
    text-align: center;
}

.select_cont .select_input h3 {
    margin: 0;
    padding: 0;
    line-height: 25px;
    font-size: 14px;
    font-weight: bold;
}

.select_cont .select_input select {
    width: 200px;
    height: 400px;
    border: 1px solid #a499a5;
    background-color: #fff;
    padding: 5px;
    background-image: none;
    box-shadow: rgba(55, 55, 55, 0.7) inset;
    margin-bottom: 10px;
}

.select_cont .select_input .btn {
    display: block;
    margin: 0 10px 10px 10px;
}

.paginate {
    display: table;
    margin: 20px auto;
    height: 30px;
    border-collapse: collapse;
}

.paginate li {
    display: table-cell;
    text-align: center;
    padding: 0 8px;
    margin: 0 2px;
    width: 1em;
    height: 1em;
    color: #666;
    font-size: 14px;
    vertical-align: middle;
    cursor: pointer;
    white-space: nowrap;
}

.paginate li.prev,
.paginate li.next,
.paginate li.more {
    width: 3em;
    border: 0 none;
}

.paginate li.first_page,
.paginate li.last_page {
    width: 2em;
    border: 0 none;
}

.paginate li.active {
    background-color: #a499a5;
    color: #fff;
}

.paginate li.active a {
    color: #fff;
}

.filter_cont {
    background-color: #f0f0f0;
    padding: 10px;
}

.filter_cont.join_form .input_div label {
    width: auto;
    white-space: nowrap;
}

.filter_cont.join_form .input_div input,
.filter_cont.join_form .input_div select {
    background-color: #fff !important;
}

.filter_cont.join_form .input_btn {
    border-top: 1px solid #a499a5;
    padding: 5px 0 0;
}

.filter_cont.join_form.inline_form .input_div {
    display: inline-block;
    float: left;
    width: auto;
}

.info_list {
    width: 100%;
    margin: 20px auto;
}

.info_list li {
    line-height: 30px;
    font-size: 14px;
    border-bottom: 1px solid #ccc;
    padding: 5px 0;
}

.info_list li i {
    width: 30px;
    margin-right: 10px;
    color: #ccc;
    font-size: 20px;
    text-align: center;
}

.info_list li .avatar {
    float: left;
    margin: 5px 10px 0 10px;
}

.info_list li .ctrl {
    float: right;
    margin-top: 30px;
    display: none;
    color: #666;
}

.info_list li a {
    color: #12bce1;
}

.info_list li p {
    margin: 0;
}

.info_list li:hover .ctrl {
    display: inline-block;
    transition: all .3s ease-in-out;
}

.info_list li.unread {
    background-color: #f0f0f0;
    font-weight: bold;
}

.avatar {
    width: 100px;
    height: 100px;
    overflow: hidden;
    border-radius: 100%;
    display: inline-block;
}

.avatar.big {
    width: 150px;
    height: 150px;
}

.avatar.small {
    width: 80px;
    height: 80px;
}

.avatar.small a {
    width: 80px;
    height: 80px;
    font-size: 14px;
    line-height: 80px;
}

.avatar.small img {
    width: 80px;
}

.avatar.tiny {
    width: 50px;
    height: 50px;
}

.avatar.tiny a {
    width: 50px;
    height: 50px;
    color: #a41b21;
    font-size: 12px;
    text-align: center;
    line-height: 50px;
}

.avatar.tiny img {
    width: 50px;
}

.avatar.tiny_reply {
    width: 40px;
    height: 40px;
}

.avatar.tiny_reply a {
    width: 40px;
    height: 40px;
}

.avatar.tiny_reply img {
    width: 40px;
}

.avatar.login {
    width: 25px;
    height: 25px;
    position: absolute;
    left: 1px;
    top: -3px;
}

.avatar.login a {
    width: 25px;
    height: 25px;
}

.avatar.login img {
    width: 25px;
    height: 25px;
}

.avatar a {
    text-align: center;
    display: inline-block;
    width: 100%;
    height: 100%;
    overflow: hidden;
    border-radius: 100%;
    box-sizing: border-box;
}

.avatar img {
    width: 100%;
    box-sizing: border-box;
}

.popup {
    display: none;
    width: 100%;
    height: 100vh;
    padding: 0;
    margin: 0 auto;
    overflow: hidden;
    position: absolute;
    top: 0;
    left: 0;
}

.popup .mask {
    width: 100%;
    height: 100vh;
    background-color: rgba(51, 51, 51, 0.5);
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1989101;
}

.popup .popup_cont {
    padding: 20px;
    border-radius: 10px;
    background-color: #fff;
    position: fixed;
    top: 50vh;
    left: 50%;
    z-index: 1989101;
}

.popup .popup_cont h2 {
    font-size: 18px;
    border-bottom: 1px solid #ccc;
    margin-top: 0;
    color: #a41b21;
    font-weight: bold;
}

.popup .popup_cont h2 span.close {
    font-size: 20px;
    width: auto;
    height: 30px;
    color: #a41b21;
    float: right;
    margin: -10px 0 0;
}

.popup .popup_cont.login_form,
.popup .popup_cont.normal {
    width: 500px;
    margin: -250px 0 0 -270px;
}

.popup .popup_cont .join_form {
    max-height: 500px;
    height: auto;
    overflow-x: hidden;
    overflow-y: auto;
}

.popup .popup_cont .foot {
    text-align: center;
    padding: 10px 0 0;
    border-top: 1px solid #12bce1;
    margin-top: 20px;
}

.tips_cont {
    width: 100%;
    font-size: 14px;
    display: none;
}

.tips_cont .warning {
    color: #a41b21;
}

.tips_cont .success {
    color: green;
}

.tips_cont ul {
    list-style: inside;
    margin-bottom: 20px;
}

.tips_cont ul li {
    list-style-type: none;
    
}

.tips_cont ul li a {
    text-decoration: underline;
}

.tags_list span {
    display: inline-block;
    padding: 3px 5px;
    background-color: #ccc;
    font-size: 12px;
    font-weight: 400;
    color: #373737;
}

.join_form {
    margin: 20px auto 0;
}

.join_form .input_div {
    margin-bottom: 10px;
    display: table;
    table-layout: fixed;
    font-size: 14px;
    width: 100%;
    line-height: 30px;
}

.join_form .input_div label {
    line-height: 25px;
    margin-bottom: 5px;
    font-size: 14px;
    font-weight: 600;
}

.join_form .input_div label em {
    font-size: 12px;
    float: right;
    color: red;
    font-weight: bold;
    font-style: normal;
}

.join_form .input_div input[type="text"],
.join_form .input_div input[type='password'],
.join_form .input_div input[type=file],
.join_form .input_div select,
.join_form .input_div textarea {
    padding: 5px;
    background: #fff;
    color: #333;
    box-sizing: border-box;
    width: 100%;
    border: 1px solid #666;
    font-size: 14px;
    border-radius: 7px;
}

.join_form .input_div input[type="text"].error,
.join_form .input_div input[type='password'].error,
.join_form .input_div input[type=file].error,
.join_form .input_div select.error,
.join_form .input_div textarea.error {
    border: 1px solid #a41b21;
    background-color: #f0f0f0;
}

.join_form .input_div input:readonly {
    background-color: #f0f0f0;
}

.join_form .input_div input[type=file] {
    display: inline-block;
    margin-right: 10px;
    width: auto;
}

.join_form .input_div select {
    width: auto;
    min-width: 7em;
    background: url("../img/arrow.png") no-repeat scroll 95% center transparent;
    padding: 0 20px 0 10px;
    line-height: 30px;
}

.join_form .input_div button[name=verify_btn] {
    width: calc((100% / 2) - 5px);
    display: inline-block;
    font-size: 16px;
    padding: 5px;
    background-color: #12bce1;
    border: 0 none;
    color: #fff;
}

.join_form .input_div .tip {
    margin: 5px;
    color: #a41b21;
    padding: 0;
    line-height: 20px;
}
#box .tip {
    margin: 5px;
    color: #a41b21;
    padding: 0;
    line-height: 20px;
    margin-left: 136px;
