/******************* EXTERNAL SCROLLBAR *******************/

.external-scroll_x {
	border: 0 none;
	cursor: pointer;
	background: #ccc;
	display: none;
	position: absolute;
}

.external-scroll_x.scroll-scrollx_visible {
	display: block;
	height: 10px;
	left: 0;
	top: 240px;
	width: 100%;
}

.external-scroll_x div,
.external-scroll_y div {
	height: 100%;
	left: 0;
	top: 0;
	position: absolute;
	width: 100%;
}

.external-scroll_x .scroll-bar {
	background: #a41b21;
	z-index: 12;
}

.external-scroll_x .scroll-bar {
	width: 100px;
}

