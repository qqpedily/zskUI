.clearfix,
.container .content,
.container .content .unit_list li.active ul.sub_list,
.container .content .unit_list li ul.sub_list,
.container .content .unit_list li .tooltips,
.container .content .right_cont .org_cont,
.container .footer,
.filter_cont,
.filter_cont.join_form .input_btn,
.filter_list,
.info_list li,
.tabs.tag_tabs ul.tab_list,
.clear {
	zoom: 1;
	clear: both;
}

.clearfix:before,
.container .content:before,
.container .content .unit_list li.active ul.sub_list:before,
.container .content .unit_list li ul.sub_list:before,
.container .content .unit_list li .tooltips:before,
.container .content .right_cont .org_cont:before,
.container .footer:before,
.filter_cont:before,
.filter_cont.join_form .input_btn:before,
.filter_list:before,
.info_list li:before,
.tabs.tag_tabs ul.tab_list:before,
.clearfix:after,
.container .content:after,
.container .content .unit_list li.active ul.sub_list:after,
.container .content .unit_list li ul.sub_list:after,
.container .content .unit_list li .tooltips:after,
.container .content .right_cont .org_cont:after,
.container .footer:after,
.filter_cont:after,
.filter_cont.join_form .input_btn:after,
.filter_list:after,
.info_list li:after,
.tabs.tag_tabs ul.tab_list:after,
.clear:before,
.clear:after {
	content: " ";
	display: table;
}

.clearfix:after,
.container .content:after,
.container .content .unit_list li.active ul.sub_list:after,
.container .content .unit_list li ul.sub_list:after,
.container .content .unit_list li .tooltips:after,
.container .content .right_cont .org_cont:after,
.container .footer:after,
.filter_cont:after,
.filter_cont.join_form .input_btn:after,
.filter_list:after,
.info_list li:after,
.tabs.tag_tabs ul.tab_list:after,
.clear:after {
	clear: both;
}

