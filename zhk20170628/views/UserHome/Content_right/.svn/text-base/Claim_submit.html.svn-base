<div class="admin_cont">
    <ul class="bread_crumbs">
        <li>所在位置：</li>
        <li>个人工作室 <i class="fa fa-angle-right"></i></li>
        <li>管理员</li>
    </ul>
    <h2>
        未提交全文
    </h2>
    <table>
        <caption>作品及贡献</caption>
        <thead>
            <tr>
                <th style="width:8%;">序号</th>
                <th style="width:25%;">作者</th>
                <th style="width:35%;">题名</th>
                <th style="width:15%;">发表时间</th>
                <th style="width:15%;">收录类型</th>
                <th style="width:25%;">操作</th>
                <th style="width:5%;">上传</th>
            </tr>
        </thead>
        <tbody ng-repeat="item in Data track by $index">
            <tr>
                <td>{{$index+1}}</td>
                <td>{{item.Author}}</td>
                <td>
                    <a href="javascript:;" ng-click="nextYe(item.ProductionID)">{{item.Title}}</a>
                </td>
                <td>{{item.CreateTime | limitTo:10}}</td>
                <td>{{item.Doctype}}</td>
                <td><input type="file" id="{{item.ProductionID}}" name="{{item.ProductionID}}" multiple="multiple" /></td>
                <td><button class="btn btn_s" type="button" ng-click="saveAll($index,'是否上传？')">上传</button></td>
            </tr>
        </tbody>
        <!--<thead>
            <tr>
                <th colspan="6"><button class="btn btn_red btn_s" type="button" ng-click="saveAll($index,'是否保存？')">保存</button></th>
            </tr>
        </thead>-->
    </table>
