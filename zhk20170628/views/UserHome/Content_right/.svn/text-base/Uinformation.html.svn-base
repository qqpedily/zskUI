			<!-- 基本信息 -->
			<div class="tab_cont tab_1" style="">
				<div class="l_statistical">
					<div class="l_statistical_div">
						<label><em>*</em>姓</label>
						<input type="text" ng-model="data.SurnameChinese" placeholder="暂无数据" />
						<!--value="{{ data.SurnameChinese}}-{{ data.NameChinese}}"-->
						<label>名</label>
						<input type="text" ng-model="data.NameChinese" placeholder="暂无数据" />
						<!--value="{{ data.NamePhoneticize}}{{ data.SurnamePhoneticize}}"-->
					</div>
					<div class="l_statistical_div">
						<label>姓</label>
						<input type="text" ng-model="data.SurnamePhoneticize" placeholder="暂无数据" />
						<label>名</label>
						<input type="text" ng-model="data.NamePhoneticize" placeholder="暂无数据" />
					</div>
					<div class="l_statistical_div">
						<label>学位</label>
						<select style="width:140px; " ng-model="data.Degree" ng-init="data.Degree=='0'">
							<option value="0" ng-selected="data.Degree=='0'">学士</option>
							<option value="1" ng-selected="data.Degree=='1'">硕士</option>
							<option value="2" ng-selected="data.Degree=='2'">博士</option>
							<option value="3" ng-selected="data.Degree=='3'">本科</option>
						</select>
						<select style="width:50px; " ng-model="data.DegreeOpen" ng-init="data.DegreeOpen=='0'">
							<option value="0" ng-selected="data.DegreeOpen=='0'">公开</option>
							<option value="1" ng-selected="data.DegreeOpen=='1'">不公开</option>
						</select>
						<label>学历</label>
						<select style="width:140px; " ng-model="data.Education" ng-init="data.Education=='1'">
							<!--<option value="">——请选择——</option>-->
							<option value="1" ng-selected="data.Education=='1'">高中</option>
							<option value="2" ng-selected="data.Education=='2'">中专</option>
							<option value="3" ng-selected="data.Education=='3'">专科</option>
							<option value="4" ng-selected="data.Education=='4'">本科</option>
							<option value="5" ng-selected="data.Education=='5'">研究生</option>
						</select>
						<select style="width:50px; " ng-model="data.EducationOpen" ng-init="data.EducationOpen ==0">
							<!--<option value="">请选择</option>-->
							<option value="0" ng-selected="data.EducationOpen=='0'">公开</option>
							<option value="1" ng-selected="data.EducationOpen=='1'">不公开</option>
						</select>
			
					</div>
					<div class="l_statistical_div ">
						<label><em>*</em>ORCID</label>
						<input type="text " ng-model="data.Orcid " placeholder="暂无数据 " />
						<span>查询/注册ORCID</span>
					</div>
					<div class="l_statistical_div ">
						<label><em>*</em>Userid</label>
						<input type="text " ng-model="data.UserID " placeholder="暂无数据 " />
						<label>ResearchID</label>
						<input type="text " ng-model="data.ResearchID " placeholder="暂无数据 " />
					</div>
					<div class="l_statistical_div ">
						<label>首页推荐</label>
						<input type="text " ng-model="data.IsRecommend " placeholder="暂无数据 " />
						<label>排序</label>
						<input type="text " ng-model="data.OrderBy " placeholder="暂无数据 " />
					</div>
					<div class="l_statistical_div ">
						<label>是否导师</label>
						<select style="width:140px; " ng-model="data.Tutor" ng-init="data.Tutor=='1'">
							<!--<option value="">——请选择——</option>-->
							<option value="1" ng-selected="data.Tutor=='1'">是</option>
							<option value="2" ng-selected="data.Tutor=='2'">否</option>
						</select>
						<select style="width:50px;" ng-model="data.TutorOpen" ng-init="data.TutorOpen=='0'">
							<!--<option value="">请选择</option>-->
							<option value="0" ng-selected="data.TutorOpen=='0'">公开</option>
							<option value="1" ng-selected="data.TutorOpen=='1'">不公开</option>
						</select>
					<label>特殊身份</label>
						<select style="width:140px; " ng-model="data.IsExpert" ng-init="data.IsExpert=='0'">
							<!--<option value="">——请选择——</option>-->
							<option value="1" ng-selected="data.IsExpert=='1'">无</option>
							<option value="0" ng-selected="data.IsExpert=='0'">专家</option>
						</select>

						<span>可以专家点评</span>
					</div>
					<div class="l_statistical_div ">
						<label>身份</label>
						<select style="width:230px;" ng-model="data.ItID">
							<!--<option value="">——请选择——</option>-->
							<option value="{{x.ItID}}" ng-repeat="x in Lbitd" >{{ x.IdentityTypeName}}</option> <!--ng-selected="data.ItID=={{x.ItID}}-->
						</select>
						<span>可以认领成果,上传成果</span>
					</div>
				</div>
				<!--<div class="text_left " style="margin-left:300px;margin-top:20px; ">
					<button type="button " class="btn btn_red add ">保存</button>
					<button type="button " class="btn btn_gray save ">还原</button>
				</div>-->
				<!--<h4 style="line-height:10px;fon-size:14px;font-weight:600; ">
                荣誉头衔
            </h4>-->
				<h4 style="line-height:10px;fon-size:14px;font-weight:600; ">
                任职信息
            </h4>
				<div class="l_statistical_div " >
					<label>性别</label>
					<input type="radio" name="female" style="width:15px;height:15px;margin-top: 10px;margin-left:20px; " value='0' ng-model='data.Sex' />
					<span>男</span>
					<input type="radio" name="female " style="width:15px;height:15px;margin-top: 10px;margin-left:80px; " value='1' ng-model='data.Sex' />
					<span>女</span>
					<label style="margin-left:74px; ">民族</label>
					<input type="text " ng-model="data.Nation " placeholder="暂无数据 " />
				</div>
				<div class="l_statistical_div ">
					<label>出生日期</label>
					<input type="text " ng-model="data.BirthDay " placeholder="暂无数据 " />
					<label>政治面貌</label>
					<input type="text " ng-model="data.PoliticalStatus " placeholder="暂无数据 " />
				</div>
				<div class="l_statistical_div ">
					<label>出生地</label>
					<input type="text " ng-model="data.BirthPlace " placeholder="暂无数据 " />
					<label>籍贯</label>
					<input type="text " ng-model="data.PlaceOrigin " placeholder="暂无数据 " />
				</div>
				<div class="input_div">
					<label>个人简介：</label>
					<textarea ng-model="data.PersonalProfile" class="textareaAll"></textarea>
				</div>
				<div class="text_left " style="margin-left:300px;margin-top:20px; ">
					<button type="button " class="btn btn_red add " ng-Click='ngSave("确认保存？")'>保存</button>
					<!--<button type="button " class="btn btn_gray save ">还原</button>-->
				</div>
				
				<!--<h4 style="line-height:10px;fon-size:14px;font-weight:600; ">
                外语能力
            </h4>
				<img src="../img/bianjiqi.png " alt=" " />
				<div class="text_left " style="margin-left:300px;margin-top:20px; ">
					<button type="button " class="btn btn_red add ">保存</button>
				</div>-->
				
			</div>
			<!-- 别名管理 -->
			<div class="tab_cont tab_2 " style="display:none; ">
				<div class="l_statistical_div ">
					<label></label>
					<input type="radio" name="setUserAliasRadio" checked="true" style="width:15px;height:15px;margin-top: 10px;margin-left:110px;" 
						ng-click="UserBatchAliasRadioClick(false)"/>
					<span>设置别名</span>
					<input type="radio" name="setUserAliasRadio" style="width:15px;height:15px;margin-top: 10px;margin-left:30px;" 
						ng-click="UserBatchAliasRadioClick(true)"/>
					<span>批量增补/重建别名</span>
				</div>
				<div class="BatchAliasRadioPanel">
					<div class="l_statistical_div ">
						<label>姓氏</label>
						<input type="text " style="width:150px;" ng-model="UserBatchAliasRadioSaveData.lastName"/>
						<span>【中文】</span>
						<input type="text " style="width:150px;" ng-model="UserBatchAliasRadioSaveData.lastNameSpell"/>
						<span>【拼音】</span>
					</div>
					<div class="l_statistical_div" id="UserBatchAliasRadio" style="display: none;">
						<label>名</label>
						<input type="text " style="width:150px; " ng-model="UserBatchAliasRadioSaveData.name"/>
						<span>【中文】</span>
						<input type="text " style="width:150px; " ng-model="UserBatchAliasRadioSaveData.nameSpell"/>
						<span>【拼音】</span>
					</div>
					<button type="button " class="btn btn_red add" style="margin: 20px 0 0 255px;" ng-click='UserBatchAliasRadioSave("确认保存？")'>保存</button>
					<div class="l_statistical_div ">
						<span style="color:red;margin-left:150px; ">注：名信息中多个汉字的拼音之间请用空格分开</span>
					</div>
				</div>
				<div class="UpdateAliasRadioPanel" style="display: none;">
					<div class="l_statistical_div">
						<label>姓氏</label>
						<input type="text " style="width:150px;" ng-model="batchAddAliaUpdateData.lastName"/>
						<span>【中文】</span>
						<input type="text " style="width:150px;" ng-model="batchAddAliaUpdateData.lastNameSpell"/>
						<span>【拼音】</span>
					</div>
					<div class="l_statistical_div">
						<label>名</label>
						<input type="text " style="width:150px; " ng-model="batchAddAliaUpdateData.name"/>
						<span>【中文】</span>
						<input type="text " style="width:150px; " ng-model="batchAddAliaUpdateData.nameSpell"/>
						<span>【拼音】</span>
					</div>
					<button type="button " class="btn btn_red add" style="margin: 20px 0 0 255px;" ng-click='UserBatchUpdateAliasRadioSave("确认保存？")'>保存</button>
					<div class="l_statistical_div ">
						<span style="color:red;margin-left:150px;display: none; ">注：名信息中多个汉字的拼音之间请用空格分开</span>
					</div>
				</div>
				
				
				<div class="text_left " style="margin-left:200px;margin-top:50px; ">
					<button type="button " class="btn btn_red add" style="margin-left: 30px;" ng-click="batchAddAlia()">批量增加别名</button>
					<button type="button " class="btn btn_gray save" ng-click="batchAddAliaFresh()">刷新</button>
				</div>
				<table>
					<thead>
						<tr>
							<th>首选中文<br />显示姓名</th>
							<th>首选英文<br />显示姓名</th>
							<th>姓名姓氏</th>
							<th>科室</th>
							<th>关键词</th>
							<th>修改</th>
							<th>删除</th>
						</tr>
					</thead>
					<tbody>
						<tr ng-repeat="userAliaItem in UserAlia">
							<td><input type="radio" name="{{userAliaItem.UUID}}" id="{{userAliaItem.UUID}}"
								ng-checked="userAliaItem.FirstChineseShow==0" ng-click="batchAddAliaBtnChange(false,$index)"/>
							</td>
							<td><input type="radio" name="{{userAliaItem.UUID}}" class="{{userAliaItem.UUID}}"
								ng-checked="userAliaItem.FirstEnglishShow==0" ng-click="batchAddAliaBtnChange(true,$index)"/>
							</td>
							<td ng-bind="userAliaItem.AliasName"></td>
							<td>血液科</td>
							<td>细胞移植</td>
							<td>
								<button type="button " class="btn btn_gray btn_s del" ng-click="batchAddAliaUpdateShow($index)">修改</button>
							</td>
							<td>
								<button type="button " class="btn btn_gray btn_s del" ng-click="batchAddAliaDel($index,'确认删除?')">删除</button>
							</td>
						</tr>
					</tbody>
				</table>
			</div>
			
			
			<!-- 学术履历 -->
			<div class="tab_cont tab_3" style="display:none;">
				<h4 style="line-height:10px;font-size:16px;font-weight:600;display: inline-block;"> 研究方向
					<!--<div class="text_left " style="margin-left:20px;margin-top:20px; display: inline-block; ">-->
						<button type="button " class="btn btn_red add " ng-click="researchDirectionAdd('确认添加研究方向?')">添加</button>
					<!--</div>-->
				</h4>

				<!--研究方向-->
				<div class="researchDirection_ceng " ng-show="flag ">
					<div class="researchDirection ">
						<span ng-click="researchDirection_hide() "></span>
						<div style="margin: 20px auto; " ng-repeat="gudata in gudata">
							<div class="l_statistical_div ">
								<label><em>*</em>学科</label>
								<input type="text " style="width:150px; " ng-model='gudata.SubjectName' />
								<span>【中文】</span>
								<input type="text " style="width:150px; " ng-model='gudata.SubjectEnglishName' />
								<span>【英文】</span>
							</div>
							<div class="l_statistical_div ">
								<label><em>*</em>研究领域</label>
								<input type="text " style="width:150px; " ng-model='gudata.SearchDirectionNam' />
								<span>【中文】</span>
								<input type="text " style="width:150px; " ng-model='gudata.SearchDirectionEng' />
								<span>【英文】</span>
							</div>
							<div class="l_statistical_div ">
								<label>主题</label>
								<input type="text " style="width:150px; " ng-model='gudata.ThemeName' />
								<span>【中文】</span>
								<input type="text " style="width:150px; " ng-model='gudata.ThemeEnglishName' />
								<span>【英文】</span>
							</div>
							<div class="l_statistical_div ">
								<label>访问权限</label>
								<select style="width:50px;" ng-model="gudata.Power" ng-init="gudata.Power==0">
									<!--<option value="">请选择</option>-->
									<option value="0" ng-selected="gudata.Power==0">公开</option>
									<option value="1" ng-selected="gudata.Power==1">内部公开</option>
									<option value="2" ng-selected="gudata.Power==2">不公开</option>
								</select>
							</div>
							<div class="text_left " style="margin-left:240px;margin-top:10px; ">
								<button type="button " class="btn btn_red add " ng-click="researchDirection($index,'确认保存修改？')">修改</button>
								<button type="button " class="btn btn_gray save " ng-click="researchDirectionDel($index,'确认删除？')">删除</button>
							</div>

						</div>
					</div>
				</div>


				<h4 style="line-height:10px;font-size:16px;font-weight:600;display: inline-block;"> 荣誉奖励
	                <!--<div class="text_left " style="margin-left:20px;margin-top:20px; display: inline-block; ">-->
						<button type="button " class="btn btn_red add " ng-click="honorAwardAdd('确认添加荣誉奖励？')">添加</button>
					<!--</div>-->
	            </h4>
				<div ng-repeat='uha in UserHonorAward'>
					<div class="l_statistical_div ">
						<label><em>*</em>获奖时间</label>
						<input type="text " style="width:380px;"  ng-model="uha.HonorAwardTime" />
					</div>
					<div class="l_statistical_div ">
						<label><em>*</em>获奖信息</label>
						<input type="text " style="width:380px; "  ng-model="uha.HonorAwardInfo"/>
						<span>【中文】</span>
		            </div>
		            <div class="l_statistical_div ">
		                <input type="text " style="width:380px;margin-left:110px; " ng-model="uha.HonorAwardInfoEnglish"/>
		                <span>【英文】</span>
					</div>
					<div class="l_statistical_div ">
						<label>说明</label>
						<input type="text " style="width:380px; " ng-model="uha.HonorAwardExplain" />
						<span>【中文】</span>
            		</div>
		            <div class="l_statistical_div ">
		                <input type="text " style="width:380px;margin-left:110px; " ng-model="uha.HonorAwardExplainEnglish"/>
		                <span>【英文】</span>
					</div>
					<div class="l_statistical_div ">
						<label>机构</label>
						<input type="text " style="width:380px;"  ng-model="uha.Mechanism" />
						<span>【中文】</span>
            		</div>
		            <div class="l_statistical_div ">
		                <input type="text " style="width:380px;margin-left:110px;" ng-model="uha.MechanismEnglish"/>
		                <span>【英文】</span>
					</div>
					<div class="l_statistical_div ">
						<label>项目名称</label>
						<input type="text " style="width:380px; " ng-model="uha.ProjectName" />
						<span>【中文】</span>
            		</div>
		            <div class="l_statistical_div ">
		                <input type="text " style="width:380px;margin-left:110px; " ng-model="uha.ProjectEnglishName"/>
		                <span>【英文】</span>
					</div>
					<div class="l_statistical_div ">
						<label>奖励类别</label>
						<select style="width:50px;" ng-model="uha.RewardType" ng-init="uha.RewardType==0">
							<!--<option value="">请选择</option>-->
							<option value="0" ng-selected="uha.RewardType==0">国家级</option>
							<option value="1" ng-selected="uha.RewardType==1">省部级</option>
							<option value="2" ng-selected="uha.RewardType==2">其他</option>
						</select>
						<label>奖励等级</label>
						<select style="width:50px;" ng-model="uha.RewardLevel" ng-init="uha.RewardLevel==0">
							<!--<option value="">请选择</option>-->
							<option value="0" ng-selected="uha.RewardLevel==0">一等奖</option>
							<option value="1" ng-selected="uha.RewardLevel==1">二等奖</option>
							<option value="2" ng-selected="uha.RewardLevel==2">三等奖</option>
						</select>
					</div>
					<div class="l_statistical_div ">
						<label>排名</label>
						<input type="text " style="width:150px; " ng-model="uha.Ranking" />
						<label>访问权限</label>
						<select style="width:50px;" ng-model="uha.Power" ng-init="uha.Power==0">
							<!--<option value="">请选择</option>-->
							<option value="0" ng-selected="uha.Power==0">公开</option>
							<option value="1" ng-selected="uha.Power==1">内部公开</option>
							<option value="2" ng-selected="uha.Power==2">不公开</option>
						</select>
					</div>
					<div class="text_left " style="margin-left:240px;margin-top:20px; margin-bottom: 30px;">
						<button type="button " class="btn btn_red add " ng-click="honorAward($index,'确认修改？')">修改</button>
						<button type="button " class="btn btn_gray save " ng-click="honorAwardDel($index,'确认删除？')">删除</button>
					</div>
				</div>

				<h4 style="line-height:10px;font-size:16px;font-weight:600; "> 研究项目
					<!--<div class="text_left " style="margin-left:20px;margin-top:20px; display: inline-block; ">-->
					<button type="button " class="btn btn_red add " ng-click="researchProjectAdd('确认添加研究项目？') ">添加</button>
            	</h4>

				<div ng-repeat='urp in UserResearchProject'>
					<div class="l_statistical_div ">
						<label><em>*</em>项目名称</label>
						<input type="text " style="width:380px; " ng-model='urp.ProjectName' />
						<span>【中文】</span>
            </div>
            <div class="l_statistical_div ">
                <input type="text " style="width:380px;margin-left:110px; " ng-model='urp.ProjectNameEnglish'/>
                <span>【英文】</span>
					</div>
					<div class="l_statistical_div ">
						<label>项目编号</label>
						<input type="text " style="width:380px; " ng-model='urp.ProjectID' />
					</div>
					<div class="l_statistical_div ">
						<label>资助机构</label>
						<input type="text " style="width:380px; " ng-model='urp.FinancialBody' />
						<span>【中文】</span>
            </div>
            <div class="l_statistical_div ">
                <input type="text " style="width:380px;margin-left:110px; " ng-model='urp.FinancialBodyEnglish'/>
                <span>【英文】</span>
					</div>
					<div class="l_statistical_div ">
						<label>依托机构</label>
						<input type="text " style="width:380px; " ng-model='urp.RelyingInstitution' />
						<span>【中文】</span>
            </div>
            <div class="l_statistical_div ">
                <input type="text " style="width:380px;margin-left:110px; " ng-model='urp.RelyingInstitutionEnglish'/>
                <span>【英文】</span>
					</div>
