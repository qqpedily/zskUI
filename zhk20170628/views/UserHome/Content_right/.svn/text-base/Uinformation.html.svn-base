<script type="text/javascript">
//	_init_area();
//	_init_area1();
//	_init_area2();
	tabs('.tabs');
	//上传附件
	var homePageInputPhoto = document.getElementById('homePageInputPhoto');
//	var homePageReminder = document.getElementById('homePageReminder');
	var homePageValidatePhoto = document.getElementById('homePageValidatePhoto');
	var isAllowFile = false;
	function validate_img(a){
		var file = a.value;
//		homePageReminder.innerText = " ";
		//判断文件格式
		//获取选件文件的全称
        var fullName = homePageInputPhoto.value;
        //截取字符
        var index = fullName.lastIndexOf('.');
        var subS = fullName.slice(index);
        //判断
        var upSubS = subS.toLocaleUpperCase();
        if(upSubS =='.JPG'||upSubS =='.GIF'||upSubS =='.PNG' || upSubS =='.JPEG'){//图片格式正确
        	//验证图片大小
//      	var image = new Image();
//   		image.src = file;
//   		var height = image.height;
//   		var width = image.width;
//      	console.log(height);
//      	console.log(width); 	
        	isAllowFile = true;
        	//图片格式正确之后调接口
        }else {//图片格式不正确
//          homePageReminder.innerText = '请上传jpg或gif格式的文件';
            layer.confirm('请上传jpg或gif格式的文件', {
	                btn: ['确认'],
	            }, function (index) {
	                layer.close(index);
	            });
        }
	}
	
	function homePagePicSend(){
		var getUserUUID = document.getElementById('getUserUUID');
		console.log(getUserUUID.innerText);
//		console.log(data.UUID);
		if(isAllowFile){
			console.log("图片上传");
			var fileObj = document.getElementById("homePageInputPhoto").files[0]; // 获取文件对象
//          var FileController = "http://192.168.199.8:8088/WBSAPI/User/SaveUserFile?useruuid="+getUserUUID.innerText+"&localpath="; // 接收上传文件的后台地址
            var FileController = BaseURL+"/user/saveuserfile?useruuid="+getUserUUID.innerText+"&localpath=";
            // FormData 对象
            var form = new FormData();
            console.log(form);
            form.append("author", "hooyes");// 可以增加表单数据
            form.append("homePageInputPhoto", fileObj);// 文件对象
            console.log(form);
            // XMLHttpRequest 对象
            var xhr = new XMLHttpRequest();
            xhr.open("post", FileController, true);
            xhr.onload = function () {
//              alert("上传完成!");
                layer.confirm('完成上传', {
	                btn: ['确认'],
	            }, function (index) {
	                layer.close(index);
	            });
            };
            xhr.send(form);
            }else{
				layer.confirm('请上传头像', {
	                btn: ['确认'],
	            }, function (index) {
	                layer.close(index);
	            });
		}
	}	
</script>
